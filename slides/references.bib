@article{Koch2020,
title = {Determining optimal input–output properties: A data-driven approach},
journal = {Automatica},
volume = {134},
pages = {109906},
year = {2021},
issn = {0005-1098},
doi = {https://doi.org/10.1016/j.automatica.2021.109906},
url = {https://www.sciencedirect.com/science/article/pii/S0005109821004295},
author = {Anne Koch and Julian Berberich and Johannes Köhler and Frank Allgöwer},
keywords = {Data-driven systems analysis, Data-based control, Input–output methods, Integral quadratic constraints, Estimation, System identification for control, Linear systems},
abstract = {Due to their relevance in systems analysis and (robust) controller design, we consider the problem of determining control-theoretic system properties of an a priori unknown system from data only. More specifically, we introduce a necessary and sufficient condition for a discrete-time linear time-invariant system to satisfy a given integral quadratic constraint (IQC) over a finite time horizon using only one input–output trajectory of finite length. Furthermore, for certain classes of IQCs, we provide convex optimization problems in form of semidefinite programs (SDPs) to retrieve the optimal, i.e. the tightest, system property description that is satisfied by the unknown system. Finally, we provide bounds on the difference between finite and infinite horizon IQCs and illustrate the effectiveness of the proposed scheme in a variety of simulation studies including noisy measurements and a high dimensional system.}
}

